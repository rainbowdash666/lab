#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    setlocale(LC_ALL, "RUS");
    float x1, x2, y1, y2, R;
    cout << "Введите координаты " << endl;
    cin >> x1 >> y1;
    cin >> x2 >> y2;
    R = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
    cout << "Расстояние между двумя точками равно " << R << endl;

    return 0;
}
