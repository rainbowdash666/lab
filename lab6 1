#include <iostream>
#include <cmath>
using namespace std;

int main()
{
	setlocale(LC_ALL, "RUS");
	cout << "Задание 1\nЗадайте 2 числа\n";
	int a, b, c;
	cout << "A= "; cin >> a;
	cout << "B= "; cin >> b;
	cout << "Было A, B: " << a << " " << b << endl;
	c = b;
	b = a;
	a = c;
	cout << "После A, B: " << a << " " << b << endl;
}
