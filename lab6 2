#include <iostream>
#include <cmath>
using namespace std;

int main()
{
	setlocale(LC_ALL, "RUS");
	printf("Задание 2\nВведите A, B, C\n");
	int a, b, c;
	cout << "A= "; cin >> a;
	cout << "B= "; cin >> b;
	cout << "C= "; cin >> c;
	cout << "Было A, B, C: " << a << " " << b << " " << c << endl;
	swap(a, b);
	swap(a, c);
	cout << "Стало A, B, C: " << a << " " << b << " " << c << endl;
	return 0;
}
